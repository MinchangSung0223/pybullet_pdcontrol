<?xml version="1.0" ?>

<robot name="onelink" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <material name="Grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="Orange">
    <color rgba="1.0 0.423529411765 0.0392156862745 1.0"/>
  </material>
  <material name="Blue">
  <color rgba="0.5 0.7 1.0 1.0"/>      
</material>
  <link name="world"/>

  <link name="link0">
    <visual>
      <geometry>
        <mesh filename="meshes/link.obj"/>
      </geometry>
      <material name="Orange"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="meshes/link.obj"/>
      </geometry>
    </collision>
    <inertial>
    <origin xyz="0 0.5 0.0"/>
      <mass value="1"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>


  <joint name="joint0" type="continuous">
    <axis xyz="1 0 0"/>
    <origin rpy="0 0 0" xyz="0 0 0.05 "/>
    <parent link="world"/>
    <child link="link0"/>

  </joint>
  


</robot>
